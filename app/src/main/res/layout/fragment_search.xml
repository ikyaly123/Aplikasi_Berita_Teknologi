        app:layout_constraintTop_toBottomOf="@+id/nama3" />



    <ImageView
        android:id="@+id/foto4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/img_mifka"
        app:layout_constraintStart_toStartOf="@+id/foto3"
        app:layout_constraintTop_toBottomOf="@+id/foto3" />

    <TextView
        android:id="@+id/nama4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:text="MIFKA"
        android:textStyle="bold"
        android:textColor="@color/light_grey"
        app:layout_constraintStart_toEndOf="@+id/foto4"
        app:layout_constraintTop_toTopOf="@+id/foto4" />

    <TextView
        android:id="@+id/stambuk4"
        android:layout_width="wrap_content"
