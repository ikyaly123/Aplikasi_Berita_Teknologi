        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:text="SHAFWAN HIDAYAT"
        android:textStyle="bold"
        android:textColor="@color/light_grey"
        app:layout_constraintStart_toEndOf="@+id/foto5"
        app:layout_constraintTop_toTopOf="@+id/foto5" />

    <TextView
        android:id="@+id/stambuk5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="F55119114"
        android:textColor="@color/light_grey"
        app:layout_constraintStart_toStartOf="@+id/nama5"
        app:layout_constraintTop_toBottomOf="@+id/nama5" />

</androidx.constraintlayout.widget.ConstraintLayout>